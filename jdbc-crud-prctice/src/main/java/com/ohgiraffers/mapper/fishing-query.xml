    <entry key="insertNewReel">
        INSERT INTO TBL_REEL
        (
            REEL_NO,
            FISHING_POINT,
            FISHING_TYPE,
            REEL_NAME,
            REEL_TYPE,
            REEL_WEIGHT
            )
        VALUES
        (
            ?, ? , ? , ? , ? , ?
        )
    </entry>
    <entry key="updateReel">
        UPDATE TBL_REEL
        SET
            FISHING_POINT = ?,
            FISHING_TYPE = ?,
            REEL_NAME = ?,
            REEL_TYPE = ?,
            REEL_WEIGHT = ?
        WHERE
            REEL_NO = ?
    </entry>
    <entry key="deleteReel">
        DELETE FROM TBL_REEL
        WHERE
            REEL_NO = ?
    </entry>




