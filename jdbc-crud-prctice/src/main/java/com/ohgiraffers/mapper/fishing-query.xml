    <entry key="selectAllFishList">
        SELECT
        fish_no
        , fish_name
        , fishing_type
        , fishing_point
        , fish_size
        FROM TBL_FISH
    </entry>

    <entry key="selectLastFishNo">
        SELECT MAX(A.fish_no)
        FROM TBL_FISH A
    </entry>

    <entry key="insertNewFish">
        INSERT INTO TBL_FISH
        (
        fish_no
        , fish_name
        , fishing_type
        , fishing_point
        , fish_size
        )
        VALUES
        (
        ?
        , ?
        , ?
        , ?
        , ?
        )
    </entry>

    <entry key="updateFish">
        UPDATE TBL_FISH
        SET fish_name = ?
        , fishing_type = ?
        , fishing_point = ?
        , fish_size = ?
        WHERE fish_no = ?
    </entry>

    <entry key="deleteFish">
        DELETE
        FROM TBL_FISH
        WHERE fish_no = ?
    </entry>


